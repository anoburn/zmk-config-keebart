    macros {
        m_sch: m_sch {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp S &kp C &kp H>;
            label = "M_SCH";
        };

        m_bt: m_bt {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&tog 6 &rgb_ug RGB_TOG &rgb_ug RGB_COLOR_HSB(249,95,53)>;
            label = "M_BT";
        };

        m_blink_color: bt_test {
            compatible = "zmk,behavior-macro-one-param";
            #binding-cells = <1>;
            bindings =
                <&macro_param_1to2 &rgb_ug RGB_COLOR_HSB_CMD MACRO_PLACEHOLDER &rgb_ug RGB_ON>,
                <&macro_wait_time 2000>,
                <&rgb_ug RGB_OFF>;

            label = "BT_TEST";
        };

        m_to_ardux: to_ardux {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&to 1 &m_blink_color RGB_COLOR_HSB_VAL(134,100,41)>;
            label = "TO_ARDUX";
        };

        m_to_qwerty: m_to_qwerty {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&to 0 &m_blink_color RGB_COLOR_HSB_VAL(31,100,46)>;
            label = "M_TO_QWERTY";
        };
    };

    keymap {
        compatible = "zmk,keymap";

        default_layer {
            display-name = "QWERTY";

            // --------------------------------------------------------------------------------
            // |  TAB |  Q  |  W  |  E  |  R  |  T  |  MUTE |  PP   |  Y  |  U   |  I  |  O  |  P  | BSPC |
            // | CTRL |  A  |  S  |  D  |  F  |  G  |  LALT |  RALT |  H  |  J   |  K  |  L  |  ;  |  '   |
            // | SHFT |  Z  |  X  |  C  |  V  |  B  |               |  N  |  M   |  ,  |  .  |  /  | ESC  |
            //                    | GUI | LWR | SPC |               | ENT | RSE  | ALT |

            bindings = <
&kp TAB    &kp Q  &kp W  &kp E     &kp R  &kp T      &none  &none  &kp Y    &kp U  &kp I      &kp O    &kp P     &kp BSPC
&kp LCTRL  &kp A  &kp S  &kp D     &kp F  &kp G      &none  &none  &kp H    &kp J  &kp K      &kp L    &kp SEMI  &kp SQT
&kp LSHFT  &kp Z  &kp X  &kp C     &kp V  &kp B                    &kp N    &kp M  &kp COMMA  &kp DOT  &kp FSLH  &kp ESC
                         &kp LGUI  &mo 1  &kp SPACE                &kp RET  &mo 2  &kp RALT
            >;

            sensor-bindings = <&inc_dec_kp C_VOL_DN C_VOL_UP &inc_dec_kp PG_UP PG_DN &inc_dec_kp C_PREV C_NEXT &inc_dec_kp C_BRI_DN C_BRI_UP>;
        };

        ardux_layer {
            display-name = "ARDUX";

            // -----------------------------------------------------------------------------------------
            // |  TAB |  1  |  2  |  3  |  4  |  5    |  LCTRL | RCTRL |  6  |  7  |  8  |  9  |  0  | BSPC |
            // | CTRL | BT1 | BT2 | BT3 | BT4 | BT5   |  LALT  | RALT  | LFT | DWN |  UP | RGT |     |      |
            // | SHFT | BTCR| RGB | RST | BOOT| Unlock|        |       |     |     |     |     |     |      |
            //                    | GUI |     | SPC   |        | ENT   |     | ALT |

            bindings = <
&none  &lt 2 O      &lt 7 T  &kp R     &lt 4 A  &none      &none  &none  &none     &lt 4 A  &kp R      &lt 7 T  &lt 2 O      &none
&none  &lt 5 SPACE  &lt 8 I  &kp S     &lt 3 E  &none      &none  &none  &none     &lt 3 E  &kp S      &lt 8 I  &lt 5 SPACE  &none
&none  &none        &none    &none     &none    &none                    &none     &none    &none      &none    &none        &none
                             &kp LGUI  &none    &kp SPACE                &kp LGUI  &none    &kp SPACE
            >;
        };

        number_layer {
            display-name = "NUMBER";

            // -----------------------------------------------------------------------------------------
            // |  TAB |  !  |  @  |  #  |  $  |  %  | LCTRL | RCTRL |  ^  |  &  |  *  |  (  |  )  | BSPC |
            // | CTRL |     |     |     |     |     |  LALT | RALT  |  -  |  =  |  [  |  ]  |  \  |  `   |
            // | SHFT |     |     |     |     |     |               |  _  |  +  |  {  |  }  | "|" |  ~   |
            //                    | GUI |     | SPC |               | ENT |     | ALT |

            bindings = <
&none  &none  &kp N3  &kp N2  &kp N1  &none   &none  &none  &none   &kp N1  &kp N2  &kp N3  &none  &none
&none  &none  &kp N6  &kp N5  &kp N4  &none   &none  &none  &none   &kp N4  &kp N5  &kp N6  &none  &none
&none  &none  &none   &none   &none   &none                 &none   &none   &none   &none   &none  &none
                      &trans  &trans  &trans                &trans  &trans  &trans
            >;
        };

        symbol_layer {
            display-name = "SYMBOL";
            bindings = <
&trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans                  &trans  &trans  &trans  &trans  &trans  &trans
                        &trans  &trans  &trans                  &trans  &trans  &trans
            >;
